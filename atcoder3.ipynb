   "source": [
    "N,M = map(int, input().split())\n",
    "A = list(map(int, input().split()))\n",
    "total_sum = sum(A)\n",
    "\n",
    "judge = False\n",
    "for i in range(N):\n",
    "    if total_sum - A[i] == M:\n",
    "        judge = True\n",
    "        break\n",
    "\n",
    "if judge:\n",
    "    print(\"Yes\")\n",
    "else:\n",
    "    print(\"No\")"
   ]
  }
