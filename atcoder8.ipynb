   "source": [
    "N = int(input())\n",
    "A = list(map(int, input().split()))\n",
    "count = 0\n",
    "for i in range(N-1):\n",
    "    for j in range(i+1,N):\n",
    "        S = sum(A[i:j+1])\n",
    "        if all(S%A[t] != 0 for t in range(i,j+1)):\n",
    "            count += 1\n",
    "print(count)"
   ]
  }
