   "source": [
    "N,K = map(int, input().split())\n",
    "S = input()\n",
    "\n",
    "list_S = list(S[n:n+K] for n in range(N-K+1))\n",
    "from collections import Counter\n",
    "dict_S = Counter(list_S)\n",
    "X = max(dict_S.values())\n",
    "print(X)\n",
    "Y = set(i for i in list_S if X == dict_S[i])\n",
    "Z = sorted(Y)\n",
    "for z in Z:\n",
    "    print(f\"{z}\", end=\" \")"
   ]
  }
