   "source": [
    "S = input()\n",
    "r = 0\n",
    "count = 0\n",
    "N = len(S)\n",
    "for l in range(N):\n",
    "    for r in range(l+1, N, 2):\n",
    "        A = S[l:r+1]\n",
    "        M =  int((r - l + 1)/2)\n",
    "        B = sorted(A)\n",
    "        if A == \"\".join(B) and len(set(A)) == 2 and int(B[M-1])+1 == int(B[M]):\n",
    "            count += 1\n",
    "print(count)"
   ]
  }
